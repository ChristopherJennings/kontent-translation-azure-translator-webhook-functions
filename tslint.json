{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [
            true,
            "never"
        ],
        "quotemark": [
            true,
            "single"
        ],
        "trailing-comma": [
            true,
            {
                "esSpecCompliant": true
            }
        ],
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "interface-name": [true, "never-prefix"]
    },
    "rulesDirectory": []
}